
@{
    Layout = null;
}
<html>
<head>

</head>
<body>

    @{Html.RenderPartial("Header", "Home"); }






        <div class="col-sm-4"></div>
        <div class="box box-success col-sm-5 text-center">
            <div class="box-header with-border">
                <h3 class="box-title linear-icon-text-align-center">Detalles Jardín</h3>
            </div>



            <div class="form-group">
                @foreach (var a in ViewBag.serv2)
                {


                    <div class="w3-row w3-section ">

                        <div class="w3-col" style="width:50px"><i class="w3-xxlarge fa fa-user"></i></div>
                        <div class="w3-rest">
                            <input class="form-control" type="text" placeholder="Ingresa el nombre del servicio" name="nombre1" disabled value="@a.NomEnc">
                        </div>
                    </div>

                    <div class="w3-row w3-section ">
                        <div class="w3-col" style="width:50px"><i class="w3-xxlarge fa fa-university"></i></div>
                        <div class="w3-rest">
                            <input class="form-control" type="text" placeholder="Ingresa el jardin" name="nombre2" disabled value="@a.NomJar">
                        </div>
                    </div>

                    <div class="w3-row w3-section ">
                        <div class="w3-col" style="width:50px"><i class="w3-xxlarge fa fa-map-marker"></i></div>
                        <div class="w3-rest">
                            <textarea class="form-control" type="text" placeholder="Ingresa la dirección" name="dir" disabled value="@a.Direccion">@a.Direccion</textarea>
                        </div>
                    </div>

                    <div class="w3-row w3-section ">
                        <div class="w3-col" style="width:50px"><i class="w3-xxlarge fa fa-phone"></i></div>
                        <div class="w3-rest">
                            <input class="form-control" type="text" placeholder="Ingresa el telefono" name="tel" disabled value="@a.Telefono">
                        </div>
                    </div>
                }
               
                <button id="btn2" class="btn btn-sm btn-success "  type="submit">Cerrar <span href="@Url.Action("indice","Home")"  class="glyphicon glyphicon-eye-close"></span></button>

            </div>
        </div>


    

</body>

</html>

